    "symptom_progression_patterns": [
      {
        "pattern": "becoming + [difficulty/hard/trouble] + [action]",
        "interpretation": "progressive function decline",
        "example": "walking becoming difficult = mobility function decline"
      },
      {
        "pattern": "can't + [action] + anymore",
        "interpretation": "complete function loss", 
        "example": "can't button shirt anymore = fine motor function loss"
      },
      {
        "pattern": "trouble + [gerund_action]",
        "interpretation": "functional difficulty",
        "example": "trouble gripping = grip function impairment"
      },
      {
        "pattern": "[body_part] + getting + [weaker/harder]",
        "interpretation": "progressive weakness",
        "example": "hands getting weaker = hand strength decline"
      }
    ],
